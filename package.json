{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@bruhno/homepage",
  "description": "My personal portfolio and playground",
  "homepage": "https://bruhno.com",
  "author": "Nicolai Bruhn Lauritsen <mail@bruhno.com> (https://bruhno.com)",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "^18"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not op_mini all",
    "node > 18"
  ],
  "scripts": {
    "build:analyze": "cross-env ANALYZE_BUILD=true next build",
    "build:debug": "next build --debug",
    "build:env": "node scripts/createEnv.js",
    "build:ladle": "ladle build",
    "build:next": "next build",
    "build:profile": "next build --profile",
    "build:sitemap": "node scripts/createSitemap.js",
    "build": "concurrently -p \"[{name}]\" -n \"next,sitemap,env,ladle\" -c \"blue,yellow,cyan,white\" -s \"all\" \"pnpm build:next\" \"pnpm build:sitemap\" \"pnpm verify:env\" \"pnpm build:ladle\"",
    "db:generate-schema": "prisma generate",
    "db:migrate": "prisma db pull",
    "debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "dev:ladle": "ladle serve",
    "dev:next": "next",
    "dev": "concurrently -p \"[{name}]\" -n \"next,ladle\" -c \"blue,white\" -s \"all\" -k \"true\" \"pnpm dev:next\" \"pnpm dev:ladle\"",
    "lint:eslint": "eslint . --ext js,mjs,ts,tsx --quiet",
    "lint:tsc": "tsc --project tsconfig.json --noEmit",
    "lint": "concurrently -p \"[{name}]\" -n \"eslint,tsc\" -c \"magenta,blue\" -s \"all\" -r \"pnpm lint:eslint\" \"pnpm lint:tsc\"",
    "postinstall": "pnpm db:generate-schema",
    "start": "next start",
    "test": "jest",
    "verify:env": "node scripts/verifyEnv.js"
  },
  "dependencies": {
    "@emotion/react": "11.11.1",
    "@hookform/resolvers": "3.1.1",
    "@nivo/core": "0.83.0",
    "@nivo/line": "0.83.0",
    "@prisma/client": "4.16.2",
    "@react-aria/focus": "3.13.0",
    "@react-aria/interactions": "3.16.0",
    "@react-hook/media-query": "1.1.1",
    "@react-three/drei": "9.78.1",
    "@react-three/fiber": "8.13.4",
    "@sentry/nextjs": "7.57.0",
    "@sentry/tracing": "7.57.0",
    "@tabler/icons-react": "2.24.0",
    "@vercel/analytics": "1.0.1",
    "argon2": "0.30.3",
    "cookie": "0.5.0",
    "date-fns": "2.30.0",
    "downshift": "7.6.0",
    "jsonwebtoken": "9.0.1",
    "lodash": "4.17.21",
    "match-sorter": "6.3.1",
    "next": "13.4.9",
    "next-connect": "0.13.0",
    "otplib": "12.0.1",
    "plaiceholder": "2.5.0",
    "qrcode": "1.5.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.45.1",
    "react-lite-youtube-embed": "2.3.52",
    "react-textarea-autosize": "8.5.2",
    "resize-observer-polyfill": "1.5.1",
    "sharp": "0.32.1",
    "superstruct": "1.0.3",
    "swr": "2.2.0",
    "three": "0.154.0",
    "ts-custom-error": "3.3.1",
    "ua-parser-js": "^1.0.35",
    "undici": "5.22.1",
    "uuid": "9.0.0",
    "zustand": "4.3.9"
  },
  "devDependencies": {
    "@emotion/eslint-plugin": "11.11.0",
    "@ladle/react": "2.15.0",
    "@next/bundle-analyzer": "13.4.9",
    "@plaiceholder/next": "2.5.0",
    "@sentry/types": "7.57.0",
    "@types/cookie": "0.5.1",
    "@types/jest": "29.5.2",
    "@types/jsonwebtoken": "9.0.2",
    "@types/lodash": "4.14.195",
    "@types/node": "20.4.1",
    "@types/qrcode": "1.5.1",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@types/supertest": "2.0.12",
    "@types/three": "0.153.0",
    "@types/ua-parser-js": "^0.7.36",
    "@types/uuid": "9.0.2",
    "@typescript-eslint/eslint-plugin": "5.61.0",
    "@typescript-eslint/parser": "5.61.0",
    "@vitejs/plugin-react": "4.0.2",
    "concurrently": "8.2.0",
    "dotenv": "16.3.1",
    "eslint": "8.44.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-next": "13.4.9",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.2",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-tsdoc": "0.2.17",
    "jest": "29.6.1",
    "jest-environment-jsdom": "29.6.1",
    "lodash-webpack-plugin": "0.11.6",
    "next-pwa": "5.6.0",
    "node-mocks-http": "1.12.2",
    "prisma": "4.16.2",
    "react-inspector": "6.0.2",
    "supertest": "6.3.3",
    "type-fest": "3.12.0",
    "typescript": "5.1.6",
    "vite": "4.4.2",
    "webpack": "5.88.1"
  }
}
