{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@bruhno/homepage",
  "description": "My personal portfolio and playground",
  "homepage": "https://bruhno.com",
  "author": "Nicolai Bruhn Lauritsen <mail@bruhno.com> (https://bruhno.com)",
  "license": "MIT",
  "private": true,
  "version": "0.0.0",
  "engines": {
    "node": "^14"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "scripts": {
    "dev": "concurrently -p \"[{name}]\" -n \"next\" -c \"blue\" -s \"all\" -k \"true\" \"pnpm dev:next\"",
    "dev:ladle": "pnpm ladle serve",
    "dev:next": "next",
    "debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "concurrently -p \"[{name}]\" -n \"next,sitemap,env\" -c \"blue,yellow,cyan\" -s \"all\" \"pnpm build:next\" \"pnpm build:sitemap\" \"pnpm verify:env\"",
    "build:env": "node scripts/createEnv.js",
    "build:next": "next build",
    "build:ladle": "pnpm ladle build",
    "build:sitemap": "node scripts/createSitemap.js",
    "build:analyze": "cross-env ANALYZE_BUILD=true next build",
    "verify:env": "node scripts/verifyEnv.js",
    "start": "next start",
    "lint": "concurrently -p \"[{name}]\" -n \"eslint,tsc\" -c \"magenta,blue\" -s \"all\" \"pnpm lint:eslint\" \"pnpm lint:tsc\"",
    "lint:eslint": "eslint . --ext js,ts,tsx",
    "lint:tsc": "tsc --project tsconfig.json --noEmit",
    "test": "jest",
    "postinstall": "prisma generate",
    "generate-schema": "prisma generate"
  },
  "dependencies": {
    "@emotion/react": "11.8.2",
    "@prisma/client": "3.11.0",
    "@react-hook/media-query": "1.1.1",
    "@sentry/nextjs": "6.19.1",
    "@sentry/tracing": "6.19.1",
    "argon2": "0.28.5",
    "cookie": "0.4.2",
    "date-fns": "2.28.0",
    "downshift": "6.1.7",
    "final-form": "4.20.6",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "match-sorter": "6.3.1",
    "next": "12.1.1-canary.15",
    "next-connect": "0.12.2",
    "otplib": "12.0.1",
    "qrcode": "1.5.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-final-form": "6.5.8",
    "react-hooks-global-state": "1.0.2",
    "react-lite-youtube-embed": "2.2.1",
    "react-textarea-autosize": "8.3.3",
    "resize-observer-polyfill": "1.5.1",
    "superstruct": "0.15.4",
    "swr": "1.2.2",
    "ts-custom-error": "3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@emotion/eslint-plugin": "11.7.0",
    "@next/bundle-analyzer": "12.1.0",
    "@sentry/types": "6.19.1",
    "@types/cookie": "0.4.1",
    "@types/jest": "27.4.1",
    "@types/jsonwebtoken": "8.5.8",
    "@types/lodash": "4.14.180",
    "@types/node": "17.0.22",
    "@types/qrcode": "1.4.2",
    "@types/react": "17.0.41",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.16.0",
    "@typescript-eslint/parser": "5.16.0",
    "concurrently": "7.0.0",
    "dotenv": "16.0.0",
    "eslint": "8.11.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-next": "12.1.0",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "26.1.2",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-tsdoc": "0.2.14",
    "isomorphic-fetch": "3.0.0",
    "jest": "27.5.1",
    "lodash-webpack-plugin": "0.11.6",
    "next-pwa": "5.4.6",
    "node-mocks-http": "1.11.0",
    "prisma": "3.11.0",
    "supertest": "6.2.2",
    "typescript": "4.6.2",
    "webpack": "5.70.0"
  }
}
