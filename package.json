{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@bruhno/homepage",
  "description": "My personal portfolio and playground",
  "homepage": "https://bruhno.com",
  "author": "Nicolai Bruhn Lauritsen <mail@bruhno.com> (https://bruhno.com)",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "^18"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "scripts": {
    "dev": "concurrently -p \"[{name}]\" -n \"next,ladle\" -c \"blue,white\" -s \"all\" -k \"true\" \"pnpm dev:next\" \"pnpm dev:ladle\"",
    "dev:ladle": "pnpm ladle serve",
    "dev:next": "next",
    "debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "concurrently -p \"[{name}]\" -n \"next,sitemap,env,ladle\" -c \"blue,yellow,cyan,white\" -s \"all\" \"pnpm build:next\" \"pnpm build:sitemap\" \"pnpm verify:env\" \"pnpm build:ladle\"",
    "build:env": "node scripts/createEnv.js",
    "build:next": "next build",
    "build:ladle": "pnpm ladle build",
    "build:sitemap": "node scripts/createSitemap.js",
    "build:analyze": "cross-env ANALYZE_BUILD=true next build",
    "verify:env": "node scripts/verifyEnv.js",
    "start": "next start",
    "lint": "concurrently -p \"[{name}]\" -n \"eslint,tsc\" -c \"magenta,blue\" -s \"all\" \"pnpm lint:eslint\" \"pnpm lint:tsc\"",
    "lint:eslint": "eslint . --ext js,ts,tsx --quiet",
    "lint:tsc": "tsc --project tsconfig.json --noEmit",
    "test": "jest",
    "postinstall": "prisma generate",
    "generate-schema": "prisma generate"
  },
  "dependencies": {
    "@emotion/react": "11.10.6",
    "@hookform/resolvers": "2.9.11",
    "@prisma/client": "4.11.0",
    "@react-aria/focus": "3.11.0",
    "@react-aria/interactions": "3.14.0",
    "@react-hook/media-query": "1.1.1",
    "@react-three/drei": "9.57.0",
    "@react-three/fiber": "8.11.5",
    "@sentry/nextjs": "7.40.0",
    "@sentry/tracing": "7.40.0",
    "@tabler/icons-react": "2.7.0",
    "@vercel/analytics": "0.1.11",
    "argon2": "0.30.3",
    "cookie": "0.5.0",
    "date-fns": "2.29.3",
    "downshift": "7.2.2",
    "jsonwebtoken": "9.0.0",
    "lodash": "4.17.21",
    "match-sorter": "6.3.1",
    "next": "13.2.3",
    "next-connect": "0.13.0",
    "otplib": "12.0.1",
    "plaiceholder": "2.5.0",
    "qrcode": "1.5.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.43.3",
    "react-lite-youtube-embed": "2.3.52",
    "react-textarea-autosize": "8.4.0",
    "resize-observer-polyfill": "1.5.1",
    "superstruct": "1.0.3",
    "swr": "2.0.4",
    "three": "0.150.1",
    "ts-custom-error": "3.3.1",
    "undici": "5.20.0",
    "zustand": "4.3.5"
  },
  "devDependencies": {
    "@emotion/eslint-plugin": "11.10.0",
    "@ladle/react": "2.9.0",
    "@next/bundle-analyzer": "13.2.3",
    "@plaiceholder/next": "2.5.0",
    "@sentry/types": "7.40.0",
    "@types/cookie": "0.5.1",
    "@types/jest": "29.4.0",
    "@types/jsonwebtoken": "9.0.1",
    "@types/lodash": "4.14.191",
    "@types/node": "18.14.5",
    "@types/qrcode": "1.5.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/supertest": "2.0.12",
    "@types/three": "0.149.0",
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.54.0",
    "@vitejs/plugin-react": "3.1.0",
    "concurrently": "7.6.0",
    "dotenv": "16.0.3",
    "eslint": "8.35.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-next": "13.2.3",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-tsdoc": "0.2.17",
    "jest": "29.4.3",
    "jest-environment-jsdom": "29.4.3",
    "lodash-webpack-plugin": "0.11.6",
    "next-pwa": "5.6.0",
    "node-mocks-http": "1.12.1",
    "prisma": "4.11.0",
    "react-inspector": "6.0.1",
    "sharp": "0.31.3",
    "supertest": "6.3.3",
    "type-fest": "3.6.1",
    "typescript": "4.9.5",
    "vite": "4.1.4",
    "webpack": "5.75.0"
  }
}