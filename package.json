{
  "name": "@bruhno/homepage",
  "description": "My personal portfolio and playground",
  "homepage": "https://bruhno.com",
  "author": "Nicolai Bruhn Lauritsen <mail@bruhno.com> (https://bruhno.com)",
  "license": "MIT",
  "private": true,
  "version": "0.0.0",
  "engines": {
    "node": "^14"
  },
  "scripts": {
    "dev": "concurrently -p \"[{name}]\" -n \"next\" -c \"blue\" -s \"all\" -k \"true\" \"yarn dev:next\"",
    "dev:next": "next",
    "debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "prebuild": "node scripts/createEnv.js",
    "build": "concurrently -p \"[{name}]\" -n \"next,sitemap,env\" -c \"blue,yellow,cyan\" -s \"all\" \"yarn build:next\" \"yarn build:sitemap\" \"yarn verify:env\"",
    "build:next": "next build",
    "build:sitemap": "node scripts/createSitemap.js",
    "build:analyze": "cross-env ANALYZE_BUILD=true next build",
    "verify:env": "node scripts/verifyEnv.js",
    "start": "next start",
    "lint": "concurrently -p \"[{name}]\" -n \"eslint,tsc\" -c \"magenta,blue\" -s \"all\" \"yarn lint:eslint\" \"yarn lint:tsc\"",
    "lint:eslint": "eslint . --ext js,ts,tsx",
    "lint:tsc": "tsc --project tsconfig.json --noEmit",
    "test": "jest"
  },
  "dependencies": {
    "@babel/core": "7.14.3",
    "@emotion/react": "11.4.0",
    "@mdi/js": "5.9.55",
    "@react-hook/media-query": "1.1.1",
    "@sentry/nextjs": "6.4.1",
    "@sentry/tracing": "^6.4.1",
    "cookie": "0.4.1",
    "date-fns": "2.21.3",
    "downshift": "6.1.3",
    "faunadb": "4.3.0",
    "final-form": "4.20.2",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "match-sorter": "6.3.0",
    "next": "10.2.2",
    "next-offline": "5.0.5",
    "otplib": "12.0.1",
    "react": "17.0.2",
    "react-autosize-textarea": "7.1.0",
    "react-dom": "17.0.2",
    "react-final-form": "6.5.3",
    "react-hooks-global-state": "1.0.1",
    "resize-observer-polyfill": "1.5.1",
    "superstruct": "0.15.2",
    "swr": "0.5.6",
    "ts-custom-error": "3.2.0"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "11.3.0",
    "@emotion/eslint-plugin": "11.2.0",
    "@next/bundle-analyzer": "10.2.2",
    "@sentry/types": "6.4.1",
    "@testing-library/react": "11.2.7",
    "@types/cookie": "0.4.0",
    "@types/jest": "26.0.23",
    "@types/jsonwebtoken": "8.5.1",
    "@types/lodash": "4.14.170",
    "@types/node": "15.6.0",
    "@types/react": "17.0.6",
    "@typescript-eslint/eslint-plugin": "4.24.0",
    "@typescript-eslint/parser": "4.24.0",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-polished": "1.1.0",
    "chalk": "4.1.1",
    "concurrently": "6.1.0",
    "dotenv": "10.0.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "eslint": "7.27.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-import": "2.23.3",
    "eslint-plugin-jest": "24.3.6",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-tsdoc": "0.2.14",
    "isomorphic-fetch": "3.0.0",
    "jest": "26.6.3",
    "lodash-webpack-plugin": "0.11.6",
    "node-mocks-http": "1.10.1",
    "react-addons-test-utils": "15.6.2",
    "react-is": "17.0.2",
    "react-test-renderer": "17.0.2",
    "typescript": "4.2.4",
    "webpack": "5.37.1"
  }
}
