{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@bruhno/homepage",
  "description": "My personal portfolio and playground",
  "homepage": "https://bruhno.com",
  "author": "Nicolai Bruhn Lauritsen <mail@bruhno.com> (https://bruhno.com)",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "^16"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "scripts": {
    "dev": "concurrently -p \"[{name}]\" -n \"next,ladle\" -c \"blue,white\" -s \"all\" -k \"true\" \"pnpm dev:next\" \"pnpm dev:ladle\"",
    "dev:ladle": "pnpm ladle serve",
    "dev:next": "next",
    "debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "concurrently -p \"[{name}]\" -n \"next,sitemap,env,ladle\" -c \"blue,yellow,cyan,white\" -s \"all\" \"pnpm build:next\" \"pnpm build:sitemap\" \"pnpm verify:env\" \"pnpm build:ladle\"",
    "build:env": "node scripts/createEnv.js",
    "build:next": "next build",
    "build:ladle": "pnpm ladle build",
    "build:sitemap": "node scripts/createSitemap.js",
    "build:analyze": "cross-env ANALYZE_BUILD=true next build",
    "verify:env": "node scripts/verifyEnv.js",
    "start": "next start",
    "lint": "concurrently -p \"[{name}]\" -n \"eslint,tsc\" -c \"magenta,blue\" -s \"all\" \"pnpm lint:eslint\" \"pnpm lint:tsc\"",
    "lint:eslint": "eslint . --ext js,ts,tsx",
    "lint:tsc": "tsc --project tsconfig.json --noEmit",
    "test": "jest",
    "postinstall": "prisma generate",
    "generate-schema": "prisma generate"
  },
  "dependencies": {
    "@emotion/react": "11.10.0",
    "@hookform/resolvers": "2.9.7",
    "@prisma/client": "4.2.1",
    "@react-aria/focus": "3.7.0",
    "@react-aria/interactions": "3.10.0",
    "@react-hook/media-query": "1.1.1",
    "@react-three/drei": "9.23.1",
    "@react-three/fiber": "8.4.1",
    "@sentry/nextjs": "7.11.1",
    "@sentry/tracing": "7.11.1",
    "argon2": "0.28.7",
    "cookie": "0.5.0",
    "date-fns": "2.29.2",
    "downshift": "6.1.9",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "match-sorter": "6.3.1",
    "next": "12.2.5",
    "next-connect": "0.13.0",
    "otplib": "12.0.1",
    "plaiceholder": "2.4.0",
    "prisma": "4.2.1",
    "qrcode": "1.5.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.34.2",
    "react-lite-youtube-embed": "2.3.1",
    "react-textarea-autosize": "8.3.4",
    "resize-observer-polyfill": "1.5.1",
    "superstruct": "0.16.0",
    "swr": "2.0.0-beta.6",
    "three": "0.143.0",
    "ts-custom-error": "3.2.0",
    "zustand": "4.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.18.10",
    "@emotion/eslint-plugin": "11.10.0",
    "@ladle/react": "2.1.2",
    "@next/bundle-analyzer": "12.2.5",
    "@plaiceholder/next": "2.4.0",
    "@sentry/types": "7.11.1",
    "@types/cookie": "0.5.1",
    "@types/jest": "28.1.7",
    "@types/jsonwebtoken": "8.5.8",
    "@types/lodash": "4.14.184",
    "@types/node": "18.7.8",
    "@types/qrcode": "1.5.0",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@types/supertest": "2.0.12",
    "@types/three": "0.143.1",
    "@typescript-eslint/eslint-plugin": "5.33.1",
    "@typescript-eslint/parser": "5.33.1",
    "@vitejs/plugin-react": "2.0.1",
    "concurrently": "7.3.0",
    "dotenv": "16.0.1",
    "eslint": "8.22.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-next": "12.2.5",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.8.4",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-tsdoc": "0.2.16",
    "isomorphic-fetch": "3.0.0",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.3",
    "lodash-webpack-plugin": "0.11.6",
    "next-pwa": "5.5.4",
    "node-mocks-http": "1.11.0",
    "sharp": "0.30.7",
    "supertest": "6.2.4",
    "type-fest": "2.18.1",
    "typescript": "4.8.1-rc",
    "vite": "3.0.9",
    "webpack": "5.74.0"
  }
}
