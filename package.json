  "scripts": {
    "dev": "concurrently -p \"[{name}]\" -n \"next,storybook\" -c \"blue,magenta\" -s \"all\" -k \"true\" \"yarn dev:next\" \"yarn dev:storybook\"",
    "dev:next": "next",
    "dev:storybook": "start-storybook -p 9000 -c .storybook --quiet --ci",
    "debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "prebuild": "node src/scripts/createEnv.js",
    "build": "concurrently -p \"[{name}]\" -n \"next,storybook,sitemap,env\" -c \"blue,magenta,yellow,cyan\" -s \"all\" \"yarn build:next\" \"yarn build:storybook\" \"yarn build:sitemap\" \"yarn verify:env\"",
    "build:next": "next build",
    "build:storybook": "build-storybook -c .storybook -o public/storybook --quiet",
    "build:sitemap": "node src/scripts/createSitemap.js",
    "build:analyze": "cross-env ANALYZE_BUILD=true next build",
    "verify:env": "node src/scripts/verifyEnv.js",
    "start": "next start",
    "lint": "concurrently -p \"[{name}]\" -n \"eslint,tsc\" -c \"magenta,blue\" -s \"all\" \"yarn lint:eslint\" \"yarn lint:tsc\"",
    "lint:eslint": "eslint . --ext js,ts,tsx,mdx",
    "lint:tsc": "tsc --project tsconfig.json --noEmit",
    "test": "jest",
    "chromatic": "npx chromatic --project-token l2q86rm59og"
  },
