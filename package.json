{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@bruhno/homepage",
  "description": "My personal portfolio and playground",
  "homepage": "https://bruhno.com",
  "author": "Nicolai Bruhn Lauritsen <mail@bruhno.com> (https://bruhno.com)",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "^18"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not op_mini all",
    "node > 18"
  ],
  "scripts": {
    "build:analyze": "cross-env ANALYZE_BUILD=true next build",
    "build:debug": "next build --debug",
    "build:env": "node scripts/createEnv.js",
    "build:ladle": "ladle build",
    "build:next": "next build",
    "build:profile": "next build --profile",
    "build:sitemap": "node scripts/createSitemap.js",
    "build": "concurrently -p \"[{name}]\" -n \"next,sitemap,env,ladle\" -c \"blue,yellow,cyan,white\" -s \"all\" \"pnpm build:next\" \"pnpm build:sitemap\" \"pnpm verify:env\" \"pnpm build:ladle\"",
    "db:generate-schema": "prisma generate",
    "db:migrate": "prisma db pull",
    "db:start": "sudo service redis-server start && sudo pg_ctlcluster 14 main start",
    "debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "dev:ladle": "ladle serve",
    "dev:next": "next",
    "dev": "concurrently -p \"[{name}]\" -n \"next,ladle\" -c \"blue,white\" -s \"all\" -k \"true\" \"pnpm dev:next\" \"pnpm dev:ladle\"",
    "lint:eslint": "eslint . --ext js,mjs,ts,tsx --quiet",
    "lint:tsc": "tsc --project tsconfig.json --noEmit",
    "lint": "concurrently -p \"[{name}]\" -n \"eslint,tsc\" -c \"magenta,blue\" -s \"all\" -r \"pnpm lint:eslint\" \"pnpm lint:tsc\"",
    "postinstall": "pnpm db:generate-schema",
    "start": "next start",
    "test": "jest",
    "verify:env": "node scripts/verifyEnv.js"
  },
  "dependencies": {
    "@emotion/react": "11.11.4",
    "@emotion/server": "^11.11.0",
    "@hookform/resolvers": "3.3.4",
    "@nivo/core": "0.85.1",
    "@nivo/line": "0.85.1",
    "@prisma/client": "5.11.0",
    "@react-aria/focus": "3.16.2",
    "@react-aria/interactions": "3.21.1",
    "@react-hook/media-query": "1.1.1",
    "@react-three/drei": "9.102.6",
    "@react-three/fiber": "8.15.19",
    "@sentry/nextjs": "7.107.0",
    "@sentry/tracing": "7.107.0",
    "@tabler/icons-react": "3.1.0",
    "@vercel/analytics": "1.2.2",
    "@vercel/kv": "^1.0.1",
    "@vercel/speed-insights": "^1.0.10",
    "argon2": "0.40.1",
    "cookie": "0.6.0",
    "date-fns": "3.5.0",
    "downshift": "8.5.0",
    "jsonwebtoken": "9.0.2",
    "lodash": "4.17.21",
    "match-sorter": "6.3.4",
    "next": "14.1.3",
    "next-connect": "0.13.0",
    "otplib": "12.0.1",
    "plaiceholder": "3.0.0",
    "qrcode": "1.5.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.51.1",
    "react-lite-youtube-embed": "2.4.0",
    "react-textarea-autosize": "8.5.3",
    "redis": "^4.6.13",
    "resize-observer-polyfill": "1.5.1",
    "sharp": "0.33.2",
    "superstruct": "1.0.4",
    "swr": "2.2.5",
    "three": "0.162.0",
    "ts-custom-error": "3.3.1",
    "undici": "6.9.0",
    "uuid": "9.0.1",
    "zustand": "4.5.2"
  },
  "devDependencies": {
    "@ducanh2912/next-pwa": "9.7.2",
    "@emotion/eslint-plugin": "11.11.0",
    "@ladle/react": "4.0.3",
    "@next/bundle-analyzer": "14.1.3",
    "@plaiceholder/next": "3.0.0",
    "@sentry/types": "7.107.0",
    "@types/cookie": "0.6.0",
    "@types/jest": "29.5.12",
    "@types/jsonwebtoken": "9.0.6",
    "@types/lodash": "4.17.0",
    "@types/node": "20.11.28",
    "@types/qrcode": "1.5.5",
    "@types/react": "18.2.66",
    "@types/react-dom": "18.2.22",
    "@types/supertest": "6.0.2",
    "@types/three": "0.162.0",
    "@types/uuid": "9.0.8",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "@vitejs/plugin-react": "4.2.1",
    "concurrently": "8.2.2",
    "dotenv": "16.4.5",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-next": "14.1.3",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.9.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-tsdoc": "0.2.17",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lodash-webpack-plugin": "0.11.6",
    "node-mocks-http": "1.14.1",
    "prisma": "5.11.0",
    "react-inspector": "6.0.2",
    "supertest": "6.3.4",
    "type-fest": "4.12.0",
    "typescript": "5.4.2",
    "vite": "4.5.0",
    "web-streams-polyfill": "^4.0.0",
    "webpack": "5.90.3"
  }
}
