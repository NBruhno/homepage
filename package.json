  "scripts": {
    "dev": "concurrently -p \"[{name}]\" -n \"next,storybook\" -c \"blue,magenta\" -s \"all\" -k \"true\" \"yarn dev:next\" \"yarn dev:storybook\"",
    "dev:next": "next",
    "dev:storybook": "start-storybook -p 9000 -c .storybook --quiet --ci",
    "prebuild": "node src/scripts/createSitemap.ts",
    "build": "concurrently -p \"[{name}]\" -n \"next,storybook\" -c \"blue,magenta\" -s \"all\" \"yarn build:next\" \"yarn build:storybook\"",
    "build:next": "next build",
    "build:storybook": "build-storybook -c .storybook -o public/storybook --quiet",
    "start": "next start",
    "lint": "concurrently -p \"[{name}]\" -n \"eslint,stylelint\" -c \"cyan,yellow\" -s \"all\" \"yarn lint:eslint\" \"yarn lint:stylelint\"",
    "lint:eslint": "eslint src/**/*.{ts,tsx,js}",
    "lint:stylelint": "stylelint src/**/*.tsx",
    "test": "echo No test implemented",
    "analyze": "cross-env ANALYZE_BUILD=true next build"
  },
